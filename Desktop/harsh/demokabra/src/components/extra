 <Grid container spacing={2}>
                    <Grid md={3} lg={3} xs={4} sm={4}></Grid>
                    <Grid md={9} lg={9} xs={12} sm={12}>
                    {userData && userData.data && userData.data.map(users=>
              <Card style={{margin:10, padding:10 ,backgroundColor:"#F0FAF5"}}>
                  <CardContent>
                      <Grid container spacing={2}>
                          <Grid md={4} lg={4} xs={12} sm={12}>
                          <img src={users.filename} height="180px" width="200px" ></img>
                          </Grid>
                          <Grid md={8} lg={8} xs={12} sm={12} style={{textAlign:'initial'}} className="vertical-align-center">
                          <Typography component="h6" variant="h6">
            {users.title}
          </Typography>  
          <Typography>
            {users.description}</Typography>    
            
            <Grid container >
                <Grid md={6} lg={6} sm={6} xs={6} style={{textAlign:'start'}}>
            <Typography > Rs. {users.price}</Typography>
                </Grid>
                <Grid md={6} lg={6} sm={6} xs={6}>
            <Typography>Rating:{users.rating}<StarRateIcon style={{verticalAlign:'bottom'}} /></Typography>
                </Grid>
            </Grid>
            <Button variant="contained" color='primary' onClick={()=>{handleData(users); handleAddedData();}}>Add To Cart</Button>
                                            </Grid>
                      </Grid>
                  </CardContent>
              </Card>
              )}
                    </Grid>

                </Grid>


                appbar previous

